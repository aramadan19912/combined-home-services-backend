# Production Environment Variables for Vercel
# These should be set in Vercel dashboard as environment variables

# Backend API URLs (replace with your Railway URLs)
VITE_API_BASE_URL=https://your-backend-app.railway.app
VITE_API_HOST_URL=https://your-backend-app.railway.app/api/v1
VITE_AUTH_SERVER_URL=https://your-backend-app.railway.app

# Environment
VITE_ENVIRONMENT=production

# Optional: Analytics and Monitoring
# VITE_SENTRY_DSN=your-sentry-dsn
# VITE_GA_MEASUREMENT_ID=your-google-analytics-id

# Optional: Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false